{
  "name": "biud-views",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "NODE_OPTIONS=--max_old_space_size=4096 nest build",
    "start:server": "nest start server --watch",
    "start:server:prd": "nest start server",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dev": "nest start esg --watch --preserveWatchOutput --parallel",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "typeorm": "ts-node --project config/tsconfig.migration.json ./node_modules/typeorm/cli.js -d config/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:show": "npm run typeorm -- migration:show",
    "migration:create": "typeorm migration:create"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.798.0",
